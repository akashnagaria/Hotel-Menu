#include <stdio.h>

int main()
{
    printf("Pick an item: \n1. Pizza  Rs 240\n2. Burger  Rs 139\n3. Pasta  Rs 179\n4. French Fries  Rs 99\n5. Sandwich  Rs 129");

    int choice = 0;  // Added missing semicolon
    scanf("%d", &choice);

    switch (choice)
    {
    case 1:
        printf("You have chosen Pizza  Rs 240");
        break;
    case 2:
        printf("You have chosen Burger  Rs 139");
        break;
    case 3:
        printf("You have chosen Pasta  Rs 179");
        break;
    case 4:
        printf("You have chosen French Fries  Rs 99");
        break;
    case 5:
        printf("You have chosen Sandwich  Rs 129");
        break;
    default:
        printf("Invalid choice");
    }

    return 0;  // Added return statement
}
