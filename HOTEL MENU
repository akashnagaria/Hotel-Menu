#include<stdio.h>
int main()
{
printf(" pick an item : \n1. Pizza  Rs 240\n2. Burger  Rs 139\n3. Pasta  Rs179\n4. French Fries  Rs99\n5. Sandwich  Rs129");
int choice=0
scanf("%d", &choice);
switch(choice)
{
case 1:
printf("you have choose Pizza  Rs240");
break;
case 2:
printf("you have choose Burger  Rs139");
break;
case 3:
printf("you have choose Pasta  Rs179");
break;
case 4:
printf("you have choose French Fries  Rs99");
break;
case 5:
printf("you have choose Sandwich");
break;
default:
printf("invalid choice");
}
}
